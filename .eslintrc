{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react", "prettier"],
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "quotes": [2, "double"],
    "react/prop-types": ["off"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
    // overrides
  }
}
